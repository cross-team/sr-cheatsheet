{
    "name": "Screen Reader Cheat Sheet",
    "version": "1.0",
    "description": "For those starting to design, develop and test for Accessibility on the mac, this is a handy cheat-sheet for a variety of screen readers.",
    "manifest_version": 2,
    "author": "Marcelo Paiva, Marcello Paiva, and Claudio Luis Vera",
    "permissions": ["activeTab"],
    "icons": {
        "16": "sr-cheatsheet-icon160.png",
        "32": "sr-cheatsheet-icon160.png",
        "48": "sr-cheatsheet-icon160.png",
        "128": "sr-cheatsheet-icon160.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["sr-cheatsheet.css"]
        }
    ],
    "browser_action": {
        "default_popup": "sr-cheatsheet.html",
        "default_title": "Screen Reader Cheat Sheet",
        "default_icon": "sr-cheatsheet-icon160.png"
    }

}